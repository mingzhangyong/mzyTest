        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DragListview" />
        <activity android:name=".divider.TBActivity" />
        <activity android:name=".viewPager.ViewPagerActivity" />
        <activity android:name=".toolbar.MyToolBarActivity" />
        <activity android:name=".coordinator.CoordinatorActivityAppBar" />
        <activity android:name=".coordinator.CoordinatorActivityFloatingButton" />
        <activity android:name=".coordinator.CoordinatorToolbar" />
        <activity android:name=".gallery.GalleryActivity" />
        <activity android:name=".myTest.ImageTest" />
    </application>

</manifest>